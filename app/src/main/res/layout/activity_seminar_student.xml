<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="br.usp.ime.seminartoken.SeminarInfoStudent">

    <TextView
        android:id="@+id/textView"
        android:layout_width="368dp"
        android:layout_height="229dp"
        android:text="TextView"
        tools:layout_editor_absoluteX="8dp"
        android:layout_marginBottom="8dp"
        app:layout_constraintBottom_toTopOf="@+id/send_attendence"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="8dp" />

    <Button
        android:id="@+id/send_attendence"
        android:layout_width="368dp"
        android:layout_height="wrap_content"
        android:layout_marginBottom="8dp"
        android:text="Enviar Confirmaçao de Presença"
        app:layout_constraintBottom_toTopOf="@+id/qr_attendence"
        tools:layout_editor_absoluteX="8dp" />

    <Button
        android:id="@+id/qr_attendence"
        android:layout_width="368dp"
        android:layout_height="wrap_content"
        android:layout_marginBottom="7dp"
        android:text="Confirmar por QR Code"
        app:layout_constraintBottom_toTopOf="parent"
        tools:layout_editor_absoluteX="8dp" />


</android.support.constraint.ConstraintLayout>


